{
  "name": "geo-quiz-tests",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "report": "npx playwright show-report",
    "install:playwright": "npx playwright install",
    "test": "npx playwright test",
    "test:f+c": "npx playwright test --project firefox --project chromium",
    "server:start": "cd ../backend && npm start",
    "build:app": "cd ../ && ./build_step.sh",
    "lint": "eslint --ext .ts .",
    "start-test-env": "docker-compose -f docker-compose.test.yml up -d",
    "stop-test-env": "docker-compose -f docker-compose.test.yml down"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.2",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "pg": "^8.11.3"
  }
}
