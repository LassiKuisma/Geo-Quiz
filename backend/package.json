{
  "name": "geo-quiz-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/index.ts",
    "start": "cross-env NODE_ENV=production node build/backend/src/index.js",
    "test": "jest --verbose --runInBand ./tests ",
    "tsc": "tsc --build",
    "lint": "eslint --ext .ts .",
    "setup:prod": "cross-env NODE_ENV=production ts-node setupData.ts",
    "setup:dev": "cross-env NODE_ENV=development ts-node setupData.ts",
    "migration:down": "ts-node src/util/rollback.ts",
    "build:front": "rm -rf build/frontend && mkdir -p build/frontend && cd ../frontend && npm run build && cp -r build/* ../backend/build/frontend",
    "build:common": "rm -rf build/common && mkdir -p build/common && cd ../common && npm run build && cp -r build/* ../backend/build/common"
  },
  "author": "Lassi Kuisma",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "sequelize": "^6.33.0",
    "umzug": "^3.3.1"
  }
}
